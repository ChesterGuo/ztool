#mobileprovision tools
	
mpinstall(){
    sh $HOME/ztool/mptools/mpinstall.sh $1
}

mputil(){
    sh $HOME/ztool/mptools/mputil.sh $1 $2
}

mpclean(){
    sh $HOME/ztool/mptools/mpclean.sh
}

mplist(){
    sh $HOME/ztool/mptools/mplist.sh
}

mpcheck(){
	sh $HOME/ztool/mptools/mpcheck.sh $1
}

mpfix(){
	sh $HOME/ztool/mptools/mpfix.sh
}